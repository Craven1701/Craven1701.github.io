        <span class="k">return</span> <span class="n">render</span><span class="p">(</span><span class="n">request</span><span class="p">,{</span>
            <span class="s">'Message'</span><span class="p">:</span> <span class="s">'No result found.'</span>
        <span class="p">})</span>
